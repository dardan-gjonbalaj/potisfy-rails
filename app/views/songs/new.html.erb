<h1>Find and add a song to your playlist or saved songs</h1>
  <%= form_for @song do |f| %>
  <%= f.label :title%><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist %><br>
  <%= f.collection_select :artist_id, Artist.all, :id, :name %><br>
  <%= f.label :genre %><br>
  <%= f.text_field :genre %><br>
  <%= f.submit "Search for song"%>
  
<%end%>