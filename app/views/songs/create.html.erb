<h1>Create new song</h1>
<%=params[:song][:artist] = Artist.find_or_create_by(name:params[:song][:artist]) %>
<%=params[:song][:genre] = Genre.find_or_create_by(genre_name:params[:song][:genre])%>
<%= @artist = Artist.find_or_create_by(name:params[:song][:artist]) %>
<%= @song = @artist.songs.build(artist_id:@artist_id, title:params[:song][:title]) %>
<%= @song.genre=Genre.find_or_create_by(genre_name:params[:song][:genre]) %>
<%= @song.save%>